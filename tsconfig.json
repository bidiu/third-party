{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": true,
    "declarationMap": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": false,
    "outDir": ".output",
    "rootDir": ".",
    "skipLibCheck": false,
    "strict": true,
    "target": "esnext",
    "types": ["node"]
  },
  "include": ["src/**/*"],
  "exclude": []
}
