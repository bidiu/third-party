{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    //   "composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": false,
    "outDir": ".output",
    "rootDir": ".",
    "skipLibCheck": false,
    "strict": true,
    "target": "esnext",
    "types": ["node"]
    //   "paths": {
    //     "@timeylo/common/*": ["packages/common/src/*"],
    //     "@timeylo/timeylo/*": ["packages/timeylo/src/*"]
    //   }
  },
  "include": ["src/**/*"],
  "exclude": []
}
